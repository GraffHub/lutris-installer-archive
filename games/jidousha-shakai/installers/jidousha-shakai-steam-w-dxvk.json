  "script": {
    "files": [
      {
        "dx2010": "https://lutris.net/files/tools/directx-2010.tar.gz"
      }
    ],
    "game": {
      "appid": 611800,
      "prefix": "$GAMEDIR/prefix",
      "arch": "win64"
    },
    "winesteam": {
      "dxvk": true,
      "overrides": {
        "xaudio2_7": "native,builtin"
      }
    },
    "installer": [
      {
        "task": {
          "description": "Creating prefix",
          "name": "create_prefix",
          "arch": "win64",
          "prefix": "$GAMEDIR/prefix"
        }
      },
      {
        "extract": {
          "dst": "$CACHE/dxsetup",
          "file": "dx2010"
        }
      },
      {
        "task": {
          "args": "/silent",
          "name": "wineexec",
          "arch": "win64",
          "executable": "$CACHE/dxsetup/DXSETUP.exe",
          "prefix": "$GAMEDIR/prefix"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR/prefix",
          "name": "winetricks",
          "arch": "win64",
          "app": "vcrun2015 corefonts"
        }
      },
      {
        "task": {
          "description": "Installing .NET 4.7.2",
          "name": "winetricks",
          "arch": "win64",
          "app": "dotnet472 win7",
          "prefix": "$GAMEDIR/prefix"
        }
      }
    ],
    "system": {
      "env": {
        "DXVK_HUD": 0
      },
      "pulse_latency": true
    }
  },
