  "script": {
    "game": {
      "exe": "drive_c/Program Files/NMSRevolution/T4C Client/T4C.exe",
      "arch": "win32",
      "prefix": "$GAMEDIR",
      "args": "--waitforpreload --nologo --nodx9ex --gc2"
