              "maxfps0": 0
            }
          },
          "file": "$GAMEDIR/drive_c/dxwnd/dxwnd.ini"
        }
      },
      {
        "task": {
          "name": "set_regedit",
          "arch": "win64",
          "key": "csmt",
          "prefix": "$GAMEDIR",
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\Direct3D\\",
          "value": "00000000",
          "type": "REG_DWORD"
        }
      },
      {
        "task": {
          "name": "set_regedit",
          "arch": "win64",
          "key": "background",
          "prefix": "$GAMEDIR",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
