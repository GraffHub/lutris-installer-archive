      }
    ],
    "game": {
      "prefix": "$GAMEDIR",
      "exe": "drive_c/SIERRA/CYBER/cyber.bat"
    },
    "installer": [
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "create_prefix",
          "arch": "win64"
        }
      },
      {
        "insert-disc": {
          "requires": "cyber/cyber.exe"
        }
      },
      {
        "merge": {
          "description": "installing Cyber Gladiators",
          "src": "$DISC/cyber",
          "dst": "$GAMEDIR/drive_c/SIERRA/CYBER"
        }
      },
      {
        "execute": {
          "command": "cd \"$GAMEDIR/drive_c/SIERRA/CYBER/\"; chmod -R u-x,g-x,o-x,u+X,u+w,g+r *;mkdir CYBERCD;"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/SIERRA/CYBER",
          "file": "nircmd"
        }
      },
      {
        "copy": {
          "dst": "$GAMEDIR/drive_c/SIERRA/CYBER",
          "src": "config"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/tmp/",
          "src": "script"
        }
      },
      {
        "copy": {
          "dst": "$GAMEDIR/drive_c/SIERRA/CYBER/",
          "src": "script2"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/tmp/mplayer/",
          "file": "mp"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/tmp/bin",
          "file": "gnusox"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/tmp/",
          "file": "grepbin"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/tmp/",
          "file": "grepdep"
        }
      },
      {
        "task": {
          "description": "Working to make this game more Linux Wine friendly",
          "name": "wineexec",
          "executable": "$GAMEDIR/drive_c/tmp/soundtrack.bat",
          "prefix": "$GAMEDIR"
        }
      },
      {
        "task": {
          "args": "/S",
          "name": "wineexec",
          "executable": "$ngl",
          "description": "NGlide installation",
          "prefix": "$GAMEDIR"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/dxwnd",
          "file": "dxwnd"
        }
      },
      {
