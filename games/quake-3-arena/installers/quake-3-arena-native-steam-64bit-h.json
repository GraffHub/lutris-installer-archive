  "script": {
    "exe": "ioquake3",
    "files": [
      {
        "highrestextures": "http://ioquake3.org/files/xcsv_hires.zip"
      },
      {
        "ioquake3installer": "http://ioquake3.org/files/1.36/installer/ioquake3-1.36-7.1.x86_64.run"
      },
      {
        "steam-data": "$STEAM:2200:baseq3"
      }
    ],
    "installer": [
      {
        "mkdir": "$GAMEDIR/bin"
      },
      {
        "chmodx": "ioquake3installer"
      },
      {
        "execute": {
          "args": "-- -i $GAMEDIR -b $GAMEDIR/bin",
          "file": "ioquake3installer"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/baseq3",
          "src": "steam-data"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/baseq3",
          "file": "$highrestextures"
        }
      }
    ]
  },
  "content": "exe: ioquake3\nfiles:\n- highrestextures: http://ioquake3.org/files/xcsv_hires.zip\n- ioquake3installer: http://ioquake3.org/files/1.36/installer/ioquake3-1.36-7.1.x86_64.run\n- steam-data: $STEAM:2200:baseq3\ninstaller:\n- mkdir: $GAMEDIR/bin\n- chmodx: ioquake3installer\n- execute:\n    args: -- -i $GAMEDIR -b $GAMEDIR/bin\n    file: ioquake3installer\n- merge:\n    dst: $GAMEDIR/baseq3\n    src: steam-data\n- extract:\n    dst: $GAMEDIR/baseq3\n    file: $highrestextures\n"
