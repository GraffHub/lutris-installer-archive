  "script": {
    "game": {
      "exe": "drive_c/windows/syswow64/wineconsole.exe",
      "args": "c:\\\\heretic2.bat",
      "prefix": "$GAMEDIR"
    },
    "custom-name": "HERETIC II - Windows CD",
    "wine": {
      "Desktop": true
    },
    "installer": [
      {
        "task": {
          "arch": "win64",
          "description": "Preparing Heretic II Windows CD wineprefix...",
          "prefix": "$GAMEDIR",
          "name": "create_prefix"
        }
      },
      {
        "insert-disc": {
          "requires": "h2_setup.exe"
        }
      },
      {
        "task": {
          "executable": "$GAMEDIR/drive_c/windows/explorer.exe",
          "args": "/desktop=HereticII,1024x780 $DISC/h2_setup.exe",
          "description": "Running setup of the game. Just proceed and keep all settings at their defaults",
          "prefix": "$GAMEDIR",
          "name": "wineexec"
        }
      },
      {
        "extract": {
          "file": "dxwnd",
          "dst": "$GAMEDIR/drive_c/dxwnd"
        }
      },
      {
        "write_config": {
          "file": "$GAMEDIR/drive_c/dxwnd/dxwnd.ini",
          "data": {
            "target": {
              "winver0": 0,
              "flago0": 0,
              "sizy0": 600,
              "country0": 0,
              "scanline0": 0,
              "flagk0": 65536,
              "flag0": 673194017,
              "filterid0": 0,
              "maxddinterface0": 7,
              "maxfps0": 0,
              "commitlength0": 0,
              "icon0": "",
              "dflagb0": 0,
              "startfolder0": "C:\\Program Files (x86)\\Heretic II\\",
              "flagq0": 0,
              "flagp0": 8192,
              "dflag0": 0,
              "flagg0": 1207959552,
              "sizx0": 800,
              "flagm0": 65536,
              "cdvol0": 64,
              "monitorid0": -1,
              "initts0": 0,
              "initresh0": 600,
              "commitaddress0": 0,
              "swapeffect0": 0,
              "initresw0": 800,
              "lang0": 0,
              "flagn0": 17825796,
              "tflag0": 0,
              "ver0": 0,
              "flagi0": 138444804,
              "path0": "C:\\Program Files (x86)\\Heretic II\\heretic2.exe",
              "slowratio0": 2,
              "launchpath0": "C:\\Program Files (x86)\\Heretic II\\heretic2.exe +set vid_ref glx +set vid_mode 3 +set vid_fullscreen 1",
              "flagj0": 4224,
              "flagl0": 0,
              "posx0": 50,
              "posy0": 50,
              "codepage0": 0,
              "flagh0": 22,
              "title0": "Heretic II",
              "renderer0": 3,
              "maxres0": -1,
              "coord0": 3
            }
          }
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp1/",
          "src": "patch107w"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp2/",
          "src": "patch106l"
        }
      },
      {
        "chmodx": "scriptcase"
      },
      {
        "execute": {
          "file": "scriptcase",
          "args": "'$GAMEDIR/drive_c/Program Files (x86)/Heretic II/' '0'"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/",
          "src": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp1/"
        }
      },
      {
        "chmodx": "scriptws"
      },
      {
        "execute": {
          "file": "scriptws",
          "args": "$RESOLUTION_WIDTH $RESOLUTION_HEIGHT"
        }
      },
      {
        "input_menu": {
          "options": [
            {
              "keep": "I prefer to use default install language"
            },
            {
              "fr": "French"
            },
            {
              "ge": "German"
            },
            {
              "it": "Italian"
            },
            {
              "sp": "Spanish"
            }
          ],
          "id": "LANG",
          "preselect": "keep",
          "description": "Please choose wich language you would like to install this game:"
        }
      },
      {
        "chmodx": "scriptlocal"
      },
      {
        "execute": {
          "file": "scriptlocal",
          "args": "$INPUT_LANG"
        }
      },
      {
        "chmodx": "scriptconf"
      },
      {
        "execute": {
          "file": "scriptconf"
        }
      },
      {
        "extract": {
          "file": "mp",
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp3/"
        }
      },
      {
        "extract": {
          "file": "gnusox",
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp3/"
        }
      },
      {
        "extract": {
          "file": "grepbin",
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp3/"
        }
      },
      {
        "extract": {
          "file": "grepdep",
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/tmp3/"
        }
      },
      {
        "task": {
          "executable": "scriptsound",
          "prefix": "$GAMEDIR",
          "name": "wineexec"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR/drive_c/",
          "src": "menu"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/",
          "src": "faq"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/gamepad profiles",
          "src": "gppreadme"
        }
      },
      {
        "copy": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/gamepad profiles",
          "src": "gppx360"
        }
      },
      {
        "copy": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Heretic II/gamepad profiles",
          "src": "gppsc"
        }
      },
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\Direct3D\\",
          "value": "00000000",
          "key": "csmt",
          "type": "REG_DWORD"
        }
      },
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\DirectSound",
          "value": "Y",
          "key": "EAXEnabled",
          "type": "REG_SZ"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR/drive_c/",
          "src": "wpp"
        }
      },
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Desktop\\",
          "value": "c:\\\\heretic2.bmp",
          "key": "wallpaper",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "value": "0 0 0",
          "key": "background",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "value": "0 0 0",
          "key": "ActiveTitle",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "value": "204 153 255",
          "key": "GradientActiveTitle",
          "type": "REG_SZ"
        }
      },
      {
        "execute": {
          "command": "cd \"$GAMEDIR/drive_c/Program Files (x86)/Heretic II/\" ; rm -f -r tmp1 tmp2 tmp3;"
        }
      }
    ],
