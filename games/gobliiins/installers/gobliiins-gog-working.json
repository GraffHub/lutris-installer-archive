      },
      {
        "execute": {
          "args": "-d \"$CACHE\" \"$file1\"",
          "file": "$CACHE/innoextract"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR",
          "src": "$CACHE"
        }
      }
    ]
  },
