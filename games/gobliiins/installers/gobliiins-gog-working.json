  "humblestoreid": "",
  "script": {
    "files": [
      {
        "file1": "N/A:Please select the GOG installer"
      },
      {
        "file2": "https://lutris.net/files/tools/innoextract.tar.gz"
      }
    ],
    "game": {
      "subtitles": true,
      "game_id": "gob1",
      "path": "$GAMEDIR"
    },
    "installer": [
      {
        "extract": {
          "file": "$file2",
          "dst": "$CACHE"
        }
      },
      {
        "execute": {
          "file": "$CACHE/innoextract",
          "args": "-d \"$CACHE\" \"$file1\""
        }
      },
      {
        "merge": {
          "src": "$CACHE",
          "dst": "$GAMEDIR"
        }
      }
    ]
  },
  "content": "files:\n- file1: N/A:Please select the GOG installer\n- file2: https://lutris.net/files/tools/innoextract.tar.gz\ngame:\n  game_id: gob1\n  path: $GAMEDIR\n  subtitles: true\ninstaller:\n- extract:\n    dst: $CACHE\n    file: $file2\n- execute:\n    args: -d \"$CACHE\" \"$file1\"\n    file: $CACHE/innoextract\n- merge:\n    dst: $GAMEDIR\n    src: $CACHE\n"
}