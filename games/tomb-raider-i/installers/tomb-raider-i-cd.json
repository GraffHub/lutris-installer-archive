  "script": {
    "game": {
      "main_file": "$GAMEDIR/tomb-play.conf"
    },
    "installer": [
      {
        "insert-disc": {
          "requires": "TOMB.EXE"
        }
      },
      {
        "task": {
          "commands": [
            "C:",
            "CD TOMBRAID",
            "TOMB.EXE"
          ],
          "drives": {
            "c": "$GAMEDIR",
            "d": "$DISC"
          },
          "path": "$GAMEDIR/tomb-play.conf",
          "name": "makeconfig"
        }
      },
      {
        "task": {
          "commands": [
            "D:",
            "INSTALL.BAT",
            "exit"
          ],
          "drives": {
            "c": "$GAMEDIR",
            "d": "$DISC"
          },
          "path": "$GAMEDIR/tomb-install.conf",
          "name": "makeconfig"
        }
      },
      {
        "task": {
          "working_dir": "$GAMEDIR",
          "config_file": "$GAMEDIR/tomb-install.conf",
          "name": "dosexec"
        }
      }
    ]
  },
