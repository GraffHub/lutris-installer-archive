  "humblestoreid": "",
  "script": {
    "custom-name": "Enclave - GOG",
    "files": [
      {
        "dxwnd": "https://freefr.dl.sourceforge.net/project/dxwnd/Latest%20build/v2_05_16_build.rar"
      },
      {
        "menu": "https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/ENCLAVE.BAT"
      },
      {
        "wpp": "https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/enclave.bmp"
      },
      {
        "gppr": "https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/gamepad%20profiles/enclave_gamepad_readme.html"
      },
      {
        "gppx360": "https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/gamepad%20profiles/enclave_x360.gamecontroller.amgp"
      },
      {
        "gppsc": "https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/gamepad%20profiles/enclave_steam.sccprofile"
      },
      {
        "inno": "https://constexpr.org/innoextract/files/innoextract-1.8-linux.tar.xz"
      },
      {
        "gog": "N/A:Please select the Enclave Windows setup exe file downloaded from GOG"
      }
    ],
    "game": {
      "prefix": "$GAMEDIR",
      "exe": "drive_c/windows/syswow64/wineconsole.exe",
      "arch": "win64",
      "args": "c:\\\\Enclave\\\\ENCLAVE.BAT"
    },
    "wine": {
      "Desktop": true
    },
    "installer": [
      {
        "task": {
          "prefix": "$GAMEDIR",
          "arch": "win64",
          "install_gecko": false,
          "install_mono": false,
          "name": "create_prefix"
        }
      },
      {
        "extract": {
          "file": "inno",
          "dst": "$CACHE/"
        }
      },
      {
        "execute": {
          "file": "$CACHE/innoextract",
          "args": "--extract $gog --output-dir $CACHE"
        }
      },
      {
        "merge": {
          "src": "$CACHE/app",
          "dst": "$GAMEDIR/drive_c/Enclave/"
        }
      },
      {
        "copy": {
          "src": "menu",
          "dst": "$GAMEDIR/drive_c/Enclave/"
        }
      },
      {
        "merge": {
          "src": "gppr",
          "dst": "$GAMEDIR/drive_c/Enclave/gamepad profiles/"
        }
      },
      {
        "copy": {
          "src": "gppx360",
          "dst": "$GAMEDIR/drive_c/Enclave/gamepad profiles/"
        }
      },
      {
        "copy": {
          "src": "gppsc",
          "dst": "$GAMEDIR/drive_c/Enclave/gamepad profiles/"
        }
      },
      {
        "copy": {
          "src": "wpp",
          "dst": "$GAMEDIR/drive_c/"
        }
      },
      {
        "extract": {
          "file": "dxwnd",
          "dst": "$GAMEDIR/drive_c/dxwnd"
        }
      },
      {
        "write_config": {
          "file": "$GAMEDIR/drive_c/dxwnd/dxwnd.ini",
          "data": {
            "target": {
              "maxy0": 0,
              "posx0": 50,
              "path0": "C:\\Enclave\\Enclave.exe",
              "inity0": 0,
              "flagm0": 0,
              "sizy0": 600,
              "maxres0": -1,
              "minx0": 0,
              "startfolder0": "C:\\Enclave\\",
              "posy0": 50,
              "launchpath0": "C:\\Enclave\\Enclave.exe",
              "initx0": 0,
              "initts0": 0,
              "expert": 1,
              "fakec0": "None",
              "scanline0": 0,
              "swapeffect0": 0,
              "monitorid0": -1,
              "slowratio0": 2,
              "tflag0": 0,
              "exepath": "C:\\Enclave\\",
              "flagq0": 0,
              "miny0": 0,
              "flagl0": 1114112,
              "flagp0": 8192,
              "initresh0": 600,
              "flagj0": 4224,
              "flagg0": 1207959568,
              "sizx0": 800,
              "flagi0": 138444804,
              "winver0": 0,
              "faked0": "None",
              "flag0": 673185825,
              "country0": 0,
              "filterid0": 0,
              "flagh0": 22,
              "dflagb0": 0,
              "maxddinterface0": 7,
              "ver0": 0,
              "flagk0": 65536,
              "dflag0": 0,
              "initresw0": 800,
              "renderer0": 3,
              "icon0": "",
              "coord0": 3,
              "title0": "Enclave",
              "cdvol0": 0,
              "maxfps0": 0,
              "lang0": 0,
              "flago0": 0,
              "codepage0": 0,
              "maxx0": 0,
              "flagn0": 17825796
            }
          }
        }
      },
      {
        "task": {
          "arch": "win64",
          "name": "set_regedit",
          "key": "wallpaper",
          "prefix": "$GAMEDIR",
          "value": "c:\\\\enclave.bmp",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Desktop\\",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "arch": "win64",
          "name": "set_regedit",
          "key": "background",
          "prefix": "$GAMEDIR",
          "value": "0 0 0",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "arch": "win64",
          "name": "set_regedit",
          "key": "ActiveTitle",
          "prefix": "$GAMEDIR",
          "value": "0 0 0",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "arch": "win64",
          "name": "set_regedit",
          "key": "GradientActiveTitle",
          "prefix": "$GAMEDIR",
          "value": "88 85 44",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "type": "REG_SZ"
        }
      },
      {
        "write_file": {
          "file": "$GAMEDIR/drive_c/Enclave/environment.cfg",
          "content": "VID_RENDER=Direct3D8\nXR_SHADOWDECALTEXTURESIZE=2048\nVID_MODE=$RESOLUTION_WIDTH $RESOLUTION_HEIGHT 32 60\nCON_ENABLE=1\n"
        }
      },
      {
        "write_file": {
          "file": "$GAMEDIR/drive_c/Enclave/options.cfg",
          "content": "SUBTITLES_OFF=1\nKB_INGAME_MENU=ESC\n"
        }
      },
      {
        "execute": {
          "command": "cd \"$GAMEDIR/drive_c/Enclave/\"; mv __support/app/Sbz1/User.cfg Sbz1/; rm -f -r GL RenderSelector gog* webcache.zip __support;"
        }
      }
    ]
  },
  "content": "custom-name: Enclave - GOG\nfiles:\n- dxwnd: https://freefr.dl.sourceforge.net/project/dxwnd/Latest%20build/v2_05_16_build.rar\n- menu: https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/ENCLAVE.BAT\n- wpp: https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/enclave.bmp\n- gppr: https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/gamepad%20profiles/enclave_gamepad_readme.html\n- gppx360: https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/gamepad%20profiles/enclave_x360.gamecontroller.amgp\n- gppsc: https://github.com/legluondunet/MyLittleLutrisScripts/raw/master/Enclave/gamepad%20profiles/enclave_steam.sccprofile\n- inno: https://constexpr.org/innoextract/files/innoextract-1.8-linux.tar.xz\n- gog: N/A:Please select the Enclave Windows setup exe file downloaded from GOG\ngame:\n  arch: win64\n  args: c:\\\\Enclave\\\\ENCLAVE.BAT\n  exe: drive_c/windows/syswow64/wineconsole.exe\n  prefix: $GAMEDIR\ninstaller:\n- task:\n    arch: win64\n    install_gecko: false\n    install_mono: false\n    name: create_prefix\n    prefix: $GAMEDIR\n- extract:\n    dst: $CACHE/\n    file: inno\n- execute:\n    args: --extract $gog --output-dir $CACHE\n    file: $CACHE/innoextract\n- merge:\n    dst: $GAMEDIR/drive_c/Enclave/\n    src: $CACHE/app\n- copy:\n    dst: $GAMEDIR/drive_c/Enclave/\n    src: menu\n- merge:\n    dst: $GAMEDIR/drive_c/Enclave/gamepad profiles/\n    src: gppr\n- copy:\n    dst: $GAMEDIR/drive_c/Enclave/gamepad profiles/\n    src: gppx360\n- copy:\n    dst: $GAMEDIR/drive_c/Enclave/gamepad profiles/\n    src: gppsc\n- copy:\n    dst: $GAMEDIR/drive_c/\n    src: wpp\n- extract:\n    dst: $GAMEDIR/drive_c/dxwnd\n    file: dxwnd\n- write_config:\n    data:\n      target:\n        cdvol0: 0\n        codepage0: 0\n        coord0: 3\n        country0: 0\n        dflag0: 0\n        dflagb0: 0\n        exepath: C:\\Enclave\\\n        expert: 1\n        fakec0: None\n        