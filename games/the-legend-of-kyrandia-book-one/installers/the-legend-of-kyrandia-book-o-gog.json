  "script": {
    "files": [
      {
        "file1": "N/A:Please select the GOG installer"
      },
      {
        "file2": "https://lutris.net/files/tools/innoextract.tar.gz"
      }
    ],
    "game": {
      "subtitles": true,
      "game_id": "kyra1",
      "path": "$GAMEDIR"
    },
    "installer": [
      {
        "extract": {
          "file": "$file2",
          "dst": "$CACHE"
        }
      },
      {
        "execute": {
          "file": "$CACHE/innoextract",
          "args": "-d \"$CACHE\" \"$file1\""
        }
      },
      {
        "merge": {
          "src": "$CACHE/app/DAT",
          "dst": "$GAMEDIR"
        }
      }
    ]
  },
