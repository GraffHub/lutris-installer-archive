  "script": {
    "game": {
      "exe": "drive_c/dxwnd/dxwnd.exe",
      "prefix": "$GAMEDIR",
      "args": "/r:0"
    },
    "files": [
      {
        "ngl": {
          "filename": "nGlide200_setup.exe",
          "url": "http://www.zeus-software.com/downloads/nglide/d-1.html"
        }
      },
      {
        "dxwnd": "https://freefr.dl.sourceforge.net/project/dxwnd/Latest%20build/v2_05_16_build.rar"
      }
    ],
    "wine": {
      "Desktop": true
    },
    "installer": [
      {
        "task": {
          "prefix": "$GAMEDIR",
          "arch": "win64",
          "name": "create_prefix"
        }
      },
      {
        "insert-disc": {
          "requires": "voodoo/spvoodoo.exe"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/splanet/",
          "src": "$DISC/splanet/"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/splanet/",
          "src": "$DISC/voodoo/"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/splanetcd/splanet/",
          "src": "$DISC/splanet/"
        }
      },
      {
        "execute": {
          "command": "cd \"$GAMEDIR/drive_c/splanet/\"; chmod -R u-x,g-x,o-x,u+X,u+w,g+r *;"
        }
      },
      {
        "write_config": {
          "data": {
            "scorch": {
              "settings": "1 0 1 1 5 95 127 0 0 5 320",
              "HighQualityVideo": 1
            }
          },
          "file": "$GAMEDIR/drive_c/splanet/splanet.ini"
        }
      },
      {
        "task": {
          "description": "NGlide installation",
          "prefix": "$GAMEDIR",
          "executable": "$ngl",
          "name": "wineexec",
          "args": "/S"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Software\\Zeus Software\\nGlide2",
          "value": "0",
          "key": "Splash",
          "arch": "win64",
          "type": "REG_SZ"
        }
      },
      {
        "extract": {
          "dst": "$GAMEDIR/drive_c/dxwnd",
          "file": "dxwnd"
        }
      },
      {
        "write_config": {
          "data": {
            "window": {
              "exepath": "C:\\splanet\\",
              "posy": 506,
              "posx": 786,
              "expert": 1,
              "exportpath": "C:\\dxwnd\\exports\\"
            },
            "target": {
              "inity0": 0,
              "flag0": 673185825,
              "ver0": 0,
              "flagn0": 17825796,
              "maxy0": 0,
              "codepage0": 0,
              "initresh0": 600,
              "notes0": null,
              "fakecddrive0": "D:",
              "dflagb0": 0,
              "module0": null,
              "exepath": "C:\\splanet\\",
              "sizy0": 600,
              "swapeffect0": 0,
              "registry0": null,
              "path0": "C:\\splanet\\spvoodoo.exe",
              "coord0": 3,
              "maxfps0": 0,
              "initx0": 0,
              "maxx0": 0,
              "flagk0": 65536,
              "launchpath0": "C:\\splanet\\spvoodoo.exe",
              "flagp0": 8192,
              "expert": 1,
              "initts0": 0,
              "maxres0": -1,
              "cdvol0": 0,
              "minx0": 0,
              "opengllib0": null,
              "fakec0": "None",
              "flagq0": 0,
              "renderer0": 3,
              "filterid0": 0,
              "scanline0": 0,
              "maxddinterface0": 7,
              "slowratio0": 2,
              "country0": 0,
              "title0": "Scorched Pnet",
              "flagm0": 0,
              "startfolder0": "C:\\splanet",
              "monitorid0": -1,
              "miny0": 0,
              "faked0": "None",
              "initresw0": 800,
              "fakecdlabel0": "SPLANET",
              "lang0": 0,
              "flago0": 64,
              "sizx0": 800,
              "flagj0": 4224,
              "flagg0": 1207959568,
              "flagl0": 1114112,
              "dflag0": 0,
              "posy0": 50,
              "tflag0": 0,
              "flagh0": 22,
              "winver0": 0,
              "icon0": "",
              "posx0": 50,
              "flagi0": 138444804,
              "fakecd0": "c:\\splanetcd"
            }
          },
          "file": "$GAMEDIR/drive_c/dxwnd/dxwnd.ini"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\Direct3D\\",
          "value": "00000000",
          "key": "csmt",
          "arch": "win64",
          "type": "REG_DWORD"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "value": "0 0 0",
          "key": "background",
          "arch": "win64",
          "type": "REG_SZ"
        }
      }
    ]
  },
  "content": "files:\n- ngl:\n    filename: nGlide200_setup.exe\n    url: http://www.zeus-software.com/downloads/nglide/d-1.html\n- dxwnd: https://freefr.dl.sourceforge.net/project/dxwnd/Latest%20build/v2_05_16_build.rar\ngame:\n  args: /r:0\n  exe: drive_c/dxwnd/dxwnd.exe\n  prefix: $GAMEDIR\ninstaller:\n- task:\n    arch: win64\n    name: create_prefix\n    prefix: $GAMEDIR\n- insert-disc:\n    requires: voodoo/spvoodoo.exe\n- merge:\n    dst: $GAMEDIR/drive_c/splanet/\n    src: $DISC/splanet/\n- merge:\n    dst: $GAMEDIR/drive_c/splanet/\n    src: $DISC/voodoo/\n- merge:\n    dst: $GAMEDIR/drive_c/splanetcd/splanet/\n    src: $DISC/splanet/\n- execute:\n    command: cd \"$GAMEDIR/drive_c/splanet/\"; chmod -R u-x,g-x,o-x,u+X,u+w,g+r *;\n- write_config:\n    data:\n      scorch:\n        HighQualityVideo: 1\n        settings: 1 0 1 1 5 95 127 0 0 5 320\n    file: $GAMEDIR/drive_c/splanet/splanet.ini\n- task:\n    args: /S\n    description: NGlide installation\n    executable: $ngl\n    name: wineexec\n    prefix: $GAMEDIR\n- task:\n    arch: win64\n    key: Splash\n    name: set_regedit\n    path: HKEY_CURRENT_USER\\Software\\Zeus Software\\nGlide2\n    prefix: $GAMEDIR\n    type: REG_SZ\n    value: '0'\n- extract:\n    dst: $GAMEDIR/drive_c/dxwnd\n    file: dxwnd\n- write_config:\n    data:\n      target:\n        cdvol0: 0\n        codepage0: 0\n        coord0: 3\n        country0: 0\n        dflag0: 0\n        dflagb0: 0\n        exepath: C:\\splanet\\\n        expert: 1\n        fakec0: None\n        fakecd0: c:\\splanetcd\n        fakecddrive0: 'D:'\n        fakecdlabel0: SPLANET\n        faked0: None\n        filterid0: 0\n        flag0: 673185825\n        flagg0: 1207959568\n        flagh0: 22\n        flagi0: 138444804\n        flagj0: 4224\n        flagk0: 65536\n        flagl0: 1114112\n        flagm0: 0\n        flagn0: 17825796\n        flago0: 64\n        flagp0: 8192\n        flagq0: 0\n        icon0: \n        initresh0: 600\n        initresw0: 800\n