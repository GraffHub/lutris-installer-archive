          "key": "aspect",
          "file": "$GAMEDIR/dosbox_mbash.conf",
          "section": "render",
          "value": true
        }
      },
      {
        "write_config": {
          "key": "fullscreen",
          "file": "$GAMEDIR/dosbox_mbash.conf",
          "section": "sdl",
          "value": false
        }
      }
    ],
    "main_file": "dosbox_mbash_single.conf"
  },
