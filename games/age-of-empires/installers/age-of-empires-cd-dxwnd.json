        }
      },
      {
        "input_menu": {
          "description": "Please precise your CD language:",
          "id": "LANG",
          "options": [
            {
              "en": "English"
            },
            {
              "fr": "French"
            },
            {
              "ge": "German"
            },
            {
              "it": "italian"
            },
            {
              "sp": "Spanish"
            },
            {
              "po": "Portuguese"
            }
          ],
          "preselect": "en"
        }
      },
      {
        "execute": {
          "command": "cd \"$GAMEDIR/drive_c/Program Files (x86)/Microsoft Games/Age of Empires\"; ls | grep -iw avi | xargs rm -fr; ls | grep -i empires.exe | xargs rm; ls | grep -i language.dll | xargs rm; cp \"$GAMEDIR/drive_c/tmp/exe/empires.exe\" . ; cp \"$GAMEDIR/drive_c/tmp/$INPUT_LANG/\"* . ;"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\Direct3D\\",
          "value": "00000000",
          "key": "csmt",
          "arch": "win64",
          "type": "REG_DWORD"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Control Panel\\Colors\\",
          "value": "0 0 0",
          "key": "background",
          "arch": "win64",
          "type": "REG_SZ"
        }
      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "app": "win7",
          "name": "winetricks"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/Program Files (x86)/Microsoft Games/Age of Empires/AVI/",
          "src": "$DISC/GAME/AVI"
        }
      }
    ]
  },
