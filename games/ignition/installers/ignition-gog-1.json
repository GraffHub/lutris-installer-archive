  "script": {
    "game": {
      "config_file": "$GAMEDIR/dosbox_igni.conf"
    },
    "files": [
      {
        "file1": "N/A:Please select the GOG installer"
      }
    ],
    "installer": [
      {
        "extract": {
          "dst": "$CACHE",
          "format": "zip",
          "file": "$file1"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR",
          "src": "$CACHE/data/noarch"
        }
      },
      {
        "write_config": {
          "section": "render",
          "value": true,
          "key": "aspect",
          "file": "$GAMEDIR/dosbox_igni.conf"
        }
      },
      {
        "write_config": {
          "section": "sdl",
          "value": false,
          "key": "fullscreen",
          "file": "$GAMEDIR/dosbox_igni.conf"
        }
      }
    ],
    "main_file": "dosbox_igni_single.conf"
  },
