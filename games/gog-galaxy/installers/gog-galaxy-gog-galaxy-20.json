  "script": {
    "files": [
      {
        "galaxy": "https://content-system.gog.com/open_link/download?path=/open/galaxy/client/setup_galaxy_2.0.14.257.exe"
      }
    ],
    "game": {
      "args": "/runWithoutUpdating /deelevated",
      "exe": "drive_c/Program Files (x86)/GOG Galaxy/GalaxyClient.exe",
      "prefix": "$GAMEDIR"
    },
    "installer": [
      {
        "task": {
          "name": "create_prefix",
          "prefix": "$GAMEDIR"
        }
      },
      {
        "task": {
          "executable": "galaxy",
          "name": "wineexec",
          "prefix": "$GAMEDIR"
        }
      },
      {
        "task": {
          "app": "physx",
          "description": "Installing needed libraries",
          "name": "winetricks",
          "prefix": "$GAMEDIR",
          "silent": true
        }
      }
    ],
    "system": {
      "env": {
        "WINEFSYNC": "1"
      },
      "reset_desktop": true,
      "restore_gamma": true
    },
    "wine": {
      "esync": true
    }
  },
  "content": "files:\n- galaxy: https://content-system.gog.com/open_link/download?path=/open/galaxy/client/setup_galaxy_2.0.14.257.exe\ngame:\n  args: /runWithoutUpdating /deelevated\n  exe: drive_c/Program Files (x86)/GOG Galaxy/GalaxyClient.exe\n  prefix: $GAMEDIR\ninstaller:\n- task:\n    name: create_prefix\n    prefix: $GAMEDIR\n- task:\n    executable: galaxy\n    name: wineexec\n    prefix: $GAMEDIR\n- task:\n    app: physx\n    description: Installing needed libraries\n    name: winetricks\n    prefix: $GAMEDIR\n    silent: true\nsystem:\n  env:\n    WINEFSYNC: '1'\n  reset_desktop: true\n  restore_gamma: true\nwine:\n  esync: true\n"
