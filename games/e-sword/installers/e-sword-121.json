      },
      {
        "task": {
          "prefix": "$GAMEDIR",
          "name": "winetricks",
          "app": "corefonts"
        }
      },
      {
        "execute": {
          "dst": "$GAMEDIR/drive_c/esword/",
          "file": "$filename"
        }
      }
    ]
  },
  "content": "files:\n- filename: https://www.e-sword.net/files/e-sword_1201_setup.exe\ngame:\n  exe: $GAMEDIR/drive_c/esword/esword.exe\n  prefix: $GAMEDIR\ninstaller:\n- task:\n    arch: win64\n    description: Creating 64bit Wine prefix.\n    name: create_prefix\n    prefix: $GAMEDIR\n- task:\n    app: corefonts\n    name: winetricks\n    prefix: $GAMEDIR\n- execute:\n    dst: $GAMEDIR/drive_c/esword/\n    file: $filename\n"
