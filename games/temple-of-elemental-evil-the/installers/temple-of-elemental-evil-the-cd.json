  "script": {
    "game": {
      "arch": "win32",
      "exe": "drive_c/Program Files/Atari/Temple of Elemental Evil/ToEE.exe",
      "prefix": "$GAMEDIR"
    },
    "installer": [
      {
        "task": {
          "name": "create_prefix",
          "prefix": "$GAMEDIR"
        }
      },
      {
        "insert-disc": {
          "description": "Insert disc",
          "requires": "setup.exe"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/CDROM",
          "src": "$DISC"
        }
      },
      {
        "execute": {
          "args": "-R 775  $GAMEDIR/drive_c/CDROM",
          "file": "/bin/chmod"
        }
      },
      {
        "insert-disc": {
          "description": "Insert disc",
          "requires": "data3.cab"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR/drive_c/CDROM",
          "src": "$DISC"
        }
      },
      {
        "execute": {
          "args": "-R 775  $GAMEDIR/drive_c/CDROM",
          "file": "/bin/chmod"
        }
      },
      {
        "task": {
          "executable": "$GAMEDIR/drive_c/CDROM/setup.exe",
          "name": "wineexec",
          "prefix": "$GAMEDIR"
        }
      },
      {
        "move": {
          "description": "Moving the CDROM files",
          "dst": "$CACHE",
          "src": "$GAMEDIR/drive_c/CDROM"
        }
      }
    ]
  },
  "content": "game:\n  arch: win32\n  exe: drive_c/Program Files/Atari/Temple of Elemental Evil/ToEE.exe\n  prefix: $GAMEDIR\ninstaller:\n- task:\n    name: create_prefix\n    prefix: $GAMEDIR\n- insert-disc:\n    description: Insert disc\n    requires: setup.exe\n- merge:\n    dst: $GAMEDIR/drive_c/CDROM\n    src: $DISC\n- execute:\n    args: -R 775  $GAMEDIR/drive_c/CDROM\n    file: /bin/chmod\n- insert-disc:\n    description: Insert disc\n    requires: data3.cab\n- merge:\n    dst: $GAMEDIR/drive_c/CDROM\n    src: $DISC\n- execute:\n    args: -R 775  $GAMEDIR/drive_c/CDROM\n    file: /bin/chmod\n- task:\n    executable: $GAMEDIR/drive_c/CDROM/setup.exe\n    name: wineexec\n    prefix: $GAMEDIR\n- move:\n    description: Moving the CDROM files\n    dst: $CACHE\n    src: $GAMEDIR/drive_c/CDROM\n"
