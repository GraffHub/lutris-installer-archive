  "script": {
    "require-binaries": "scons,g++",
    "files": [
      {
        "source": "https://github.com/endless-sky/endless-sky/archive/v0.9.8.tar.gz"
      }
    ],
    "exe": "endless-sky",
    "installer": [
      {
        "extract": {
          "dst": "$GAMEDIR/src",
          "file": "source"
        }
      },
      {
        "execute": {
          "command": "cd $GAMEDIR/src && scons -j$(nproc)"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR",
          "src": "$GAMEDIR/src/endless-sky"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR",
          "src": "$GAMEDIR/src/data"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR",
          "src": "$GAMEDIR/src/images"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR",
          "src": "$GAMEDIR/src/sounds"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR",
          "src": "$GAMEDIR/src/keys.txt"
        }
      },
      {
        "move": {
          "dst": "$GAMEDIR",
          "src": "$GAMEDIR/src/credits.txt"
        }
      },
      {
        "execute": {
          "command": "rm -rf $GAMEDIR/src"
        }
      }
    ]
  },
  "content": "exe: endless-sky\nfiles:\n- source: https://github.com/endless-sky/endless-sky/archive/v0.9.8.tar.gz\ninstaller:\n- extract:\n    dst: $GAMEDIR/src\n    file: source\n- execute:\n    command: cd $GAMEDIR/src && scons -j$(nproc)\n- move:\n    dst: $GAMEDIR\n    src: $GAMEDIR/src/endless-sky\n- move:\n    dst: $GAMEDIR\n    src: $GAMEDIR/src/data\n- move:\n    dst: $GAMEDIR\n    src: $GAMEDIR/src/images\n- move:\n    dst: $GAMEDIR\n    src: $GAMEDIR/src/sounds\n- move:\n    dst: $GAMEDIR\n    src: $GAMEDIR/src/keys.txt\n- move:\n    dst: $GAMEDIR\n    src: $GAMEDIR/src/credits.txt\n- execute:\n    command: rm -rf $GAMEDIR/src\nrequire-binaries: scons,g++\n"
