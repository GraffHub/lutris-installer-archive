  "script": {
    "game": {
      "exe": "drive_c/Program Files (x86)/Ubisoft/Ubisoft Game Launcher/UbisoftGameLauncher.exe",
      "arch": "win64",
      "prefix": "$GAMEDIR"
    },
    "system": {
      "env": {
        "DXVK_HUD": 0
      }
    },
    "wine": {
      "overrides": {
        "xaudio2_7": "native,builtin"
      },
      "dxvk": true
    },
    "installer": [
      {
        "task": {
          "arch": "win64",
          "prefix": "$GAMEDIR",
          "name": "create_prefix"
        }
      },
      {
        "task": {
          "app": "corefonts",
          "prefix": "$GAMEDIR",
          "name": "winetricks"
        }
      },
      {
        "task": {
          "executable": "uplay",
          "args": "/silent",
          "prefix": "$GAMEDIR",
          "name": "wineexec"
        }
      }
    ],
    "files": [
      {
        "uplay": "https://ubistatic3-a.akamaihd.net/orbit/launcher_installer/UplayInstaller.exe"
      }
    ]
  },
