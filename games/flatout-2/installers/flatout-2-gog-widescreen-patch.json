  "script": {
    "files": [
      {
        "patch": "https://lutris.net/files/games/flatout-2/Flatout2_widescreen_fix.zip"
      }
    ],
    "installer": [
      {
        "extract": {
          "dst": "$GAMEDIR/Game/prefix/drive_c/GOG Games/FlatOut 2/",
          "file": "$patch"
        }
      },
      {
        "task": {
          "key": "dinput8",
          "name": "set_regedit",
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\DllOverrides",
          "prefix": "$GAMEDIR/Game/prefix",
          "value": "native,builtin"
        }
      }
    ],
    "requires": "flatout-2-gog"
  },
  "content": "files:\n- patch: https://lutris.net/files/games/flatout-2/Flatout2_widescreen_fix.zip\ninstaller:\n- extract:\n    dst: $GAMEDIR/Game/prefix/drive_c/GOG Games/FlatOut 2/\n    file: $patch\n- task:\n    key: dinput8\n    name: set_regedit\n    path: HKEY_CURRENT_USER\\Software\\Wine\\DllOverrides\n    prefix: $GAMEDIR/Game/prefix\n    value: native,builtin\nrequires: flatout-2-gog\n"
