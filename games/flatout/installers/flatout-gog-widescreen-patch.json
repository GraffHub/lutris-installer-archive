  "script": {
    "extends": "flatout-gog",
    "files": [
      {
        "patch": "https://github.com/ThirteenAG/WidescreenFixesPack/releases/download/flatout/Flatout.WidescreenFix.zip"
      }
    ],
    "installer": [
      {
        "extract": {
          "dst": "$GAMEDIR/prefix/drive_c/GOG Games/FlatOut/",
          "file": "$patch"
        }
      },
      {
        "task": {
          "path": "HKEY_CURRENT_USER\\Software\\Wine\\DllOverrides",
          "prefix": "$GAMEDIR/prefix",
          "key": "dinput8",
          "name": "set_regedit",
          "value": "native,builtin"
        }
      }
    ]
  },
  "content": "extends: flatout-gog\nfiles:\n- patch: https://github.com/ThirteenAG/WidescreenFixesPack/releases/download/flatout/Flatout.WidescreenFix.zip\ninstaller:\n- extract:\n    dst: $GAMEDIR/prefix/drive_c/GOG Games/FlatOut/\n    file: $patch\n- task:\n    key: dinput8\n    name: set_regedit\n    path: HKEY_CURRENT_USER\\Software\\Wine\\DllOverrides\n    prefix: $GAMEDIR/prefix\n    value: native,builtin\n"
