  "script": {
    "files": [
      {
        "GOG_INSTALLER": "N/A:Please select GOG installer file"
      },
      {
        "INNO_INSTALLER": "https://constexpr.org/innoextract/files/innoextract-1.8/innoextract-1.8-linux.tar.xz"
      }
    ],
    "game": {
      "appid": 496760,
      "game_id": "gk2",
      "gog_id": 120765883,
      "path": "$GAMEDIR"
    },
    "installer": [
      {
        "extract": {
          "dst": "$CACHE",
          "file": "$INNO_INSTALLER"
        }
      },
      {
        "execute": {
          "args": "-d \"$CACHE\" \"$GOG_INSTALLER\"",
          "file": "$CACHE/innoextract"
        }
      },
      {
        "merge": {
          "dst": "$GAMEDIR",
          "src": "$CACHE"
        }
      }
    ]
  },
  "content": "files:\n- GOG_INSTALLER: N/A:Please select GOG installer file\n- INNO_INSTALLER: https://constexpr.org/innoextract/files/innoextract-1.8/innoextract-1.8-linux.tar.xz\ngame:\n  appid: 496760\n  game_id: gk2\n  gog_id: 120765883\n  path: $GAMEDIR\ninstaller:\n- extract:\n    dst: $CACHE\n    file: $INNO_INSTALLER\n- execute:\n    args: -d \"$CACHE\" \"$GOG_INSTALLER\"\n    file: $CACHE/innoextract\n- merge:\n    dst: $GAMEDIR\n    src: $CACHE\n"
